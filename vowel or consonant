ch=input()
if ((ch=='a')or(ch=='A')or(ch=='e')or(ch=='E')or(ch=='I')or(ch=='i')or(ch=='o')or(ch=='O')or(ch=='u')or(ch=='U')):
    print("Vowel.")
elif((ord(ch)>=65)and(ord(ch)<=90))or((ord(ch)<=97)and(ord(ch)>=122)):
    if((ch!='a')or(ch!='A')or(ch!='e')or(ch!='E')or(ch!='I')or(ch!='i')or(ch!='o')or(ch!='O')or(ch!='u')or(ch!='U')):
        print("Consonant.")
else:
    print("Not an alphabet.")
